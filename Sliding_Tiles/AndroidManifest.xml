<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sliding_tiles"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GameMenu"
            android:label="@string/title_activity_game_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NumberMode"
            android:label="@string/title_activity_number_mode" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sliding_tiles" />
        </activity>
        <activity
            android:name=".MathMode"
            android:label="@string/title_activity_math_mode" >
           
        </activity>
    </application>

</manifest>